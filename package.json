{
  "name": "fast-poke",
  "version": "0.1.1",
  "private": "true",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon",
    "debug": "nodemon --config nodemon-debug.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "build": "rimraf dist && tsc",
    "prod": "cross-env NODE_ENV=production node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "fastify": "^3.27.2",
    "http-status": "^1.5.0",
    "uuid": "^8.3.2",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
